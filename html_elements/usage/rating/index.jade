include /html_elements/lib/main

-
  var title = 'rating | Usage'

+html
  +head
    +title
  +body
    +page
      .page__gutter

        h1
          | rating

        dl
          dt
            code.code
              | +rating
          dd.div
            p.u-font-large
              +rating

        dl
          dt
            code.code
              | +rating(value="5" stars="7")
          dd.div
            p.u-font-large
              +rating(value="5" stars="7")

        dl
          dt
            code.code
              | +rating(value="5" readonly="true")
          dd.div
            p.u-font-large
              +rating(value="5" readonly="true")
