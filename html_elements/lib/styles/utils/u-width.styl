.u-width

  &-full:not(:root):not(:root)
    width 100%

  &-half:not(:root):not(:root)
    width 50%

  &-third:not(:root):not(:root)
    width 33%

  &-quarter:not(:root):not(:root)
    width 25%

  for num in 1..100

    &-{num}0px:not(:root):not(:root)
      width num * 10px

  for value in "768px"

    &-{value}:not(:root):not(:root)
      width unquote(value)
